<!-- <%= debug @awarding_contract %> -->
<%= simple_form_for(@awarding_contract, html: { multipart: true }) do |f| %>
    <%= f.input :title %>
    <%= f.input :description, as:"text" %>
    <%= f.input :procurement_method_id, collection: @procurement_methods %>
    <%= f.input :attachment_doc, as: :file %>
    <% if @awarding_contract.new_record? %>
      <%= f.submit t('form.button.create'), class: 'btn btn-primary'%>
    <% else %>
      <%= f.submit t('form.button.update'), class: 'btn btn-primary'%>
    <% end %>
<% end %>

