<%= render 'website/header/header' %>
<!-- content -->      
<div class="content  col-md-8 body">

    <div class="has-padding has-shadow" style="margin-bottom:10px;" >
      <b><%= "Bidding Advertisment "  %></b>
    </div>
    <br>
    <div class="box">
      <div class="box-body">
        <table border="1" id="my_datatable" class="table table-striped table-bordered dataTable" style="font-size: 13px;" >
          <thead>
            <tr>
              <th >#</th>
              <th >បរិយាយអំពីការដេញថ្លៃ</th>
              <th width="170px" >ស្ថាប័នអនុវត្តិលទ្ធកម្ម</th>
              <th >ប្រភពមូលនិធិ</th>
            </tr>
          </thead>
          <tbody  >

            <%
            $i = 1
            $max = 100
            while $i< $max do
            %>

              <tr  >
                <td style="vertical-align: middle; text-align:center"><%= $i %></td>
                <td style="line-height: 24px;" >
                <%= link_to "សេចក្តីជូនដំណឹងជាសាធារណៈស្តីពីការដាក់ឲ្យដេញថ្លៃផ្គត់ផ្គង់ ឧបករណ៍ប្រព័ន្ធត្រួតពិនិត្យសម្ពាធចំនួន៥គ្រឿង" , "" %><br>
                <%= "សេចក្តីជូនដំណឹង៖" %>[<%= link_to "ភាសាខ្មែរ", "" %>]
                <br>
                <%= "ដំណឹងដេញថ្លៃ៖" %>[<%= link_to "ភាសាខ្មែរ", "" %>]
                <br>
                <i><%= "Last updated on July 14, 2014" %></i>
                </td>
                <td style="vertical-align:middle; line-height: 24px;" ><%= "រដ្ឋាករទឹកស្វយ័តក្រុងភ្នំពេញ" %></td>
                <td style="vertical-align:middle; line-height: 24px;" ><%= "ថវិការបស់រដ្ឋាករទឹកស្វយ័តក្រុងភ្នំពេញ" %></td>

            <%
            $i +=1
            end

            %>
            <tr  >
                <td style="vertical-align: middle; text-align:center"><%= $i %></td>
                <td style="line-height: 24px;" >
                <%= link_to "សេចក្តីជូនដំណឹងជាសាធារណៈស្តីពីការដាក់ឲ្យដេញថ្លៃផ្គត់ផ្គង់ ឧបករណ៍ប្រព័ន្ធត្រួតពិនិត្យសម្ពាធចំនួន៥គ្រឿង" , "" %><br>
                <%= "សេចក្តីជូនដំណឹង៖" %>[<%= link_to "ភាសាខ្មែរ", "" %>]
                <br>
                <%= "ដំណឹងដេញថ្លៃ៖" %>[<%= link_to "ភាសាខ្មែរ", "" %>]
                <br>
                <i><%= "Last updated on July 15, 2014" %></i>
                </td>
                <td style="vertical-align:middle; line-height: 24px;" ><%= "រដ្ឋាករទឹកស្វយ័តក្រុងភ្នំពេញ" %></td>
                <td style="vertical-align:middle; line-height: 24px;" ><%= "ថវិការបស់រដ្ឋាករទឹកស្វយ័តក្រុងភ្នំពេញ" %></td>

          </tbody>
          
        </table>
      </div><!-- /.box-body -->
    </div><!-- /.box -->

</div>
<!-- /.content -->
<%= render'website/sidebars/right_sidebar' %>
<%= render 'website/footer/footer' %>